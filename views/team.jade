extends layout


block content
    .container
        h1= "Meet the Team"
        p Here are some dudes: #{title}
        if (team.length > 0)
            .team-wrap
            each teamMember in team
                .team-member.col-sm-4
                    h3= teamMember.tag
                    p= teamMember.fullName
                    p= teamMember.position
                    p= teamMember.nationality
                    img.img-thumbnail(src='/images/player-pic/#{teamMember.image}')

        else
            p Team Does Not EXIST!
